{
    "name": "altis/ab-test",
    "varyOn": "attributes.eventVariantId.keyword",
    "settings": {
        "category": "altis-experience-blocks",
        "icon": "lightbulb",
        "supports": {
            "alignWide": true,
            "html": false,
            "align": true
        },
        "attributes": {
            "clientId": {
                "type": "string",
                "required": true
            },
            "end": {
                "type": "string",
                "format": "date-time"
            },
            "paused": {
                "type": "boolean",
                "default": false
            },
            "percentage": {
                "type": "number",
                "default": 100,
                "minimum": 0,
                "maximum": 100
            },
            "start": {
                "type": "string",
                "format": "date-time"
            },
            "started": {
                "type": "boolean",
                "default": true
            },
            "title": {
                "type": "string"
            },
            "winner": {
                "type": "number"
            }
        }
    }
}
